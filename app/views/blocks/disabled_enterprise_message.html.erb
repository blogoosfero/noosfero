<% message = block.owner.environment.message_for_disabled_enterprise || '' %>

<div id='enterprise-disabled'>
  <%= message %>
  <% if profile.blocked? && user && @user_is_admin %>
    <div class='unlock-button'>
    <%= button :lock, _('Unblock'), {:controller => 'profile', :action => 'unblock'} %>
    </div>
  <% end %>
</div>
